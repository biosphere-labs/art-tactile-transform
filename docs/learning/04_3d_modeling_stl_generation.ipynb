    "def demonstrate_mesh_generation():\n",
    "    \"\"\"Show how height grids are converted to triangle meshes.\"\"\"\n",
    "    \n",
    "    print(\"üî∫ Mesh Generation and Triangulation\")\n",
    "    print(\"=\"*40)\n",
    "    \n",
    "    # Create a simple 3x3 heightmap for demonstration\n",
    "    simple_heightmap = np.array([\n",
    "        [0.0, 0.2, 0.1],\n",
    "        [0.3, 0.8, 0.4],\n",
    "        [0.1, 0.3, 0.0]\n",
    "    ])\n",
    "    \n",
    "    print(\"üìä Example 3x3 Heightmap:\")\n",
    "    print(simple_heightmap)\n",
    "    \n",
    "    # Show grid cell subdivision\n",
    "    print(\"\\nüîß Grid Cell Triangulation Strategy:\")\n",
    "    print(\"Each grid cell (quad) is divided into two triangles:\")\n",
    "    print(\"\")\n",
    "    print(\"Grid cell vertices:    Triangle subdivision:\")\n",
    "    print(\"(i,j)     (i,j+1)      (i,j)     (i,j+1)\")\n",
    "    print(\"  +----------+            +----------+\")\n",
    "    print(\"  |          |            |\\\\         |\")\n",
    "    print(\"  |   QUAD   |     ‚Üí      | \\\\  T2    |\")\n",
    "    print(\"  |          |            |  \\\\       |\")\n",
    "    print(\"  +----------+            | T1 \\\\     |\")\n",
    "    print(\"(i+1,j)   (i+1,j+1)      +------\\\\---+\")\n",
    "    print(\"                        (i+1,j)   (i+1,j+1)\")\n",
    "    \n",
    "    # Calculate number of triangles\n",
    "    height, width = simple_heightmap.shape\n",
    "    num_quads = (height - 1) * (width - 1)\n",
    "    num_triangles = num_quads * 2\n",
    "    \n",
    "    print(f\"\\nüìà Triangle Count Calculation:\")\n",
    "    print(f\"   Heightmap size: {height} x {width}\")\n",
    "    print(f\"   Number of quads: ({height}-1) x ({width}-1) = {num_quads}\")\n",
    "    print(f\"   Number of triangles: {num_quads} x 2 = {num_triangles}\")\n",
    "    \n",
    "    # Show specific triangle generation\n",
    "    print(f\"\\nüîç Example Triangle Generation:\")\n",
    "    print(\"For grid cell at (0,0):\")\n",
    "    \n",
    "    # Get corner heights\n",
    "    z00 = simple_heightmap[0, 0]  # (0,0)\n",
    "    z10 = simple_heightmap[1, 0]  # (1,0)\n",
    "    z01 = simple_heightmap[0, 1]  # (0,1)\n",
    "    z11 = simple_heightmap[1, 1]  # (1,1)\n",
    "    \n",
    "    print(f\"   Corner heights: z00={z00}, z10={z10}, z01={z01}, z11={z11}\")\n",
    "    \n",
    "    # With pixel scale of 1.0 for simplicity\n",
    "    pixel_scale = 1.0\n",
    "    \n",
    "    print(f\"\\n   Triangle 1 vertices:\")\n",
    "    print(f\"      v1: ({0 * pixel_scale}, {0 * pixel_scale}, {z00})\")\n",
    "    print(f\"      v2: ({1 * pixel_scale}, {0 * pixel_scale}, {z10})\")\n",
    "    print(f\"      v3: ({1 * pixel_scale}, {1 * pixel_scale}, {z11})\")\n",
    "    \n",
    "    print(f\"\\n   Triangle 2 vertices:\")\n",
    "    print(f\"      v1: ({0 * pixel_scale}, {0 * pixel_scale}, {z00})\")\n",
    "    print(f\"      v2: ({1 * pixel_scale}, {1 * pixel_scale}, {z11})\")\n",
    "    print(f\"      v3: ({0 * pixel_scale}, {1 * pixel_scale}, {z01})\")\n",
    "    \n",
    "    return simple_heightmap, num_triangles\n",
    "\n",
