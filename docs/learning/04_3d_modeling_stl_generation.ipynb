    "            (max_x, 0, base_z),\n",
    "            auto_normal=False\n",
    "        )\n",
    "        self.normals[-1] = np.array([0, 0, -1])  # Downward normal\n",
    "        \n",
    "        self.add_triangle(\n",
    "            (0, 0, base_z),\n",
    "            (0, max_y, base_z),\n",
    "            (max_x, max_y, base_z),\n",
    "            auto_normal=False\n",
    "        )\n",
    "        self.normals[-1] = np.array([0, 0, -1])  # Downward normal\n",
    "        \n",
    "        print(f\"   Added {2} base triangles\")\n",
    "    \n",
    "    def add_side_walls(self, heightmap, physical_params):\n",
    "        \"\"\"Add side walls for a completely enclosed model.\"\"\"\n",
    "        \n",
    "        print(f\"üîß Adding side walls...\")\n",
    "        \n",
    "        height, width = heightmap.shape\n",
    "        pixel_scale = physical_params['pixel_scale_mm']\n",
    "        min_height = physical_params['min_height_mm']\n",
    "        max_height = physical_params['max_height_mm']\n",
