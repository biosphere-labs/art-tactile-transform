    "def demonstrate_physical_scaling():\n",
    "    \"\"\"Demonstrate the mathematics of physical scaling.\"\"\"\n",
    "    \n",
    "    print(\"üßÆ Physical Scaling Mathematics\")\n",
    "    print(\"=\"*35)\n",
    "    \n",
    "    # Example parameters\n",
    "    heightmap_size = (64, 64)  # pixels\n",
    "    pixel_scale_mm = 0.2       # mm per pixel\n",
    "    min_height_mm = 0.5        # minimum height\n",
    "    max_height_mm = 3.0        # maximum height\n",
    "    base_thickness_mm = 1.0    # base plate thickness\n",
    "    \n",
    "    print(f\"üìä Example Configuration:\")\n",
    "    print(f\"   Heightmap size: {heightmap_size[0]} x {heightmap_size[1]} pixels\")\n",
    "    print(f\"   Pixel scale: {pixel_scale_mm} mm/pixel\")\n",
    "    print(f\"   Height range: {min_height_mm} - {max_height_mm} mm\")\n",
    "    print(f\"   Base thickness: {base_thickness_mm} mm\")\n",
    "    \n",
    "    # Calculate physical dimensions\n",
    "    physical_width = heightmap_size[0] * pixel_scale_mm\n",
    "    physical_height = heightmap_size[1] * pixel_scale_mm\n",
    "    total_model_height = max_height_mm + base_thickness_mm\n",
    "    \n",
    "    print(f\"\\nüìê Resulting Physical Model:\")\n",
    "    print(f\"   Width: {physical_width} mm\")\n",
    "    print(f\"   Depth: {physical_height} mm\")\n",
    "    print(f\"   Total height: {total_model_height} mm\")\n",
    "    print(f\"   Volume (approx): {physical_width * physical_height * (total_model_height/2):.1f} mm¬≥\")\n",
    "    \n",
    "    # Coordinate transformation formulas\n",
    "    print(f\"\\nüî¢ Coordinate Transformation Formulas:\")\n",
    "    print(\"   Pixel (i, j) ‚Üí Physical (x, y):\")\n",
    "    print(f\"      x = i * {pixel_scale_mm}\")\n",
    "    print(f\"      y = j * {pixel_scale_mm}\")\n",
    "    \n",
    "    print(\"\\n   Normalized height [0,1] ‚Üí Physical z:\")\n",
    "    print(f\"      z = height_norm * ({max_height_mm} - {min_height_mm}) + {min_height_mm} + {base_thickness_mm}\")\n",
    "    print(f\"      z = height_norm * {max_height_mm - min_height_mm} + {min_height_mm + base_thickness_mm}\")\n",
    "    \n",
    "    # Example transformations\n",
    "    print(f\"\\nüéØ Example Transformations:\")\n",
    "    examples = [\n",
    "        (0, 0, 0.0),    # Bottom-left corner, minimum height\n",
    "        (32, 32, 0.5),  # Center, medium height\n",
    "        (63, 63, 1.0),  # Top-right corner, maximum height\n",
    "    ]\n",
    "    \n",
    "    for i, j, height_norm in examples:\n",
    "        x = i * pixel_scale_mm\n",
    "        y = j * pixel_scale_mm\n",
    "        z = height_norm * (max_height_mm - min_height_mm) + min_height_mm + base_thickness_mm\n",
    "        \n",
    "        print(f\"   Pixel ({i:2d}, {j:2d}, {height_norm:.1f}) ‚Üí Physical ({x:4.1f}, {y:4.1f}, {z:.1f}) mm\")\n",
    "    \n",
    "    return {\n",
    "        'physical_width': physical_width,\n",
    "        'physical_height': physical_height,\n",
    "        'total_height': total_model_height\n",
    "    }\n",
