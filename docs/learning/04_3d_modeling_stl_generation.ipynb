    "            edge1 = np.array(v2) - np.array(v1)\n",
    "            edge2 = np.array(v3) - np.array(v1)\n",
    "            area = 0.5 * np.linalg.norm(np.cross(edge1, edge2))\n",
    "            \n",
    "            # Keep triangles with non-zero area\n",
    "            if area > 1e-10:\n",
    "                valid_triangles.append(triangle)\n",
    "                valid_normals.append(normal)\n",
    "        \n",
    "        self.triangles = valid_triangles\n",
    "        self.normals = valid_normals\n",
    "        \n",
    "        removed_count = original_count - len(self.triangles)\n",
    "        print(f\"   Removed {removed_count} degenerate triangles\")\n",
    "        print(f\"   Final triangle count: {len(self.triangles)}\")\n",
    "    \n",
    "    def export_stl(self, filename, binary=False):\n",
    "        \"\"\"Export the mesh to STL format.\"\"\"\n",
    "        \n",
    "        print(f\"üíæ Exporting to {filename}...\")\n",
    "        \n",
    "        if binary:\n",
    "            self._export_binary_stl(filename)\n",
    "        else:\n",
    "            self._export_ascii_stl(filename)\n",
    "    \n",
    "    def _export_ascii_stl(self, filename):\n",
    "        \"\"\"Export ASCII STL file.\"\"\"\n",
    "        \n",
    "        with open(filename, 'w') as f:\n",
    "            f.write(\"solid advanced_tactile_model\\n\")\n",
    "            \n",
    "            for triangle, normal in zip(self.triangles, self.normals):\n",
