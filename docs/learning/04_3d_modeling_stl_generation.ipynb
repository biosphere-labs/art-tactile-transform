   "source": [
    "def demonstrate_normal_calculation():\n",
    "    \"\"\"Demonstrate surface normal calculation mathematics.\"\"\"\n",
    "    \n",
    "    print(\"üìê Surface Normal Calculations\")\n",
    "    print(\"=\"*35)\n",
    "    \n",
    "    print(\"üßÆ Mathematical Foundation:\")\n",
    "    print(\"Surface normals are calculated using the cross product of two edge vectors.\")\n",
    "    print(\"\")\n",
    "    print(\"For a triangle with vertices A, B, C:\")\n",
    "    print(\"   Edge vector 1: AB = B - A\")\n",
    "    print(\"   Edge vector 2: AC = C - A\")\n",
    "    print(\"   Normal vector: N = AB √ó AC (cross product)\")\n",
    "    print(\"   Unit normal: NÃÇ = N / |N| (normalize to length 1)\")\n",
    "    \n",
    "    # Example calculation\n",
    "    print(\"\\nüî¢ Example Calculation:\")\n",
    "    \n",
    "    # Define triangle vertices\n",
    "    A = np.array([0.0, 0.0, 1.0])\n",
    "    B = np.array([1.0, 0.0, 1.5])\n",
    "    C = np.array([0.0, 1.0, 0.8])\n",
    "    \n",
    "    print(f\"   Vertex A: {A}\")\n",
    "    print(f\"   Vertex B: {B}\")\n",
    "    print(f\"   Vertex C: {C}\")\n",
    "    \n",
    "    # Calculate edge vectors\n",
    "    AB = B - A\n",
    "    AC = C - A\n",
    "    \n",
    "    print(f\"\\n   Edge AB: {AB}\")\n",
    "    print(f\"   Edge AC: {AC}\")\n",
    "    \n",
    "    # Calculate cross product\n",
    "    normal = np.cross(AB, AC)\n",
    "    print(f\"\\n   Cross product AB √ó AC: {normal}\")\n",
    "    \n",
    "    # Normalize\n",
    "    normal_length = np.linalg.norm(normal)\n",
    "    unit_normal = normal / normal_length if normal_length > 0 else np.array([0, 0, 1])\n",
    "    \n",
    "    print(f\"   Normal length: {normal_length:.4f}\")\n",
    "    print(f\"   Unit normal: {unit_normal}\")\n",
    "    \n",
    "    # Show how our calculate_normals function works\n",
    "    print(\"\\nüîß Our Implementation:\")\n",
    "    print(inspect.getsource(calculate_normals))\n",
    "    \n",
    "    # Test with actual grid points\n",
    "    print(\"\\nüß™ Testing with Grid Heights:\")\n",
    "    \n",
    "    # Simulate grid cell heights\n",
    "    z00, z10, z01, z11 = 1.0, 1.5, 0.8, 1.2\n",
    "    pixel_scale = 0.2\n",
    "    \n",
    "    calculated_normal = calculate_normals(z00, z10, z01, z11, 0, 0, pixel_scale)\n",
    "    \n",
    "    print(f\"   Grid heights: z00={z00}, z10={z10}, z01={z01}, z11={z11}\")\n",
    "    print(f\"   Pixel scale: {pixel_scale} mm\")\n",
    "    print(f\"   Calculated normal: {calculated_normal}\")\n",
    "    \n",
    "    return unit_normal, calculated_normal\n",
    "\n",
    "# Demonstrate normal calculations\n",
    "example_normal, grid_normal = demonstrate_normal_calculation()\n",
    "\n",
    "print(\"\\nüéØ Why Surface Normals Matter:\")\n",
    "importance = [\n",
    "    \"3D rendering: Correct lighting and shading\",\n",
    "    \"3D printing: Proper support structure generation\",\n",
    "    \"Slicer software: Layer orientation and infill patterns\",\n",
    "    \"Quality control: Detect inverted or problematic surfaces\",\n",
    "    \"CAD software: Mesh validation and repair\"\n",
    "]\n",
    "\n",
    "for importance_item in importance:\n",
    "    print(f\"‚Ä¢ {importance_item}\")\n",
    "\n",
    "print(\"\\n‚ö†Ô∏è Common Normal Issues:\")\n",
    "issues = [\n",
    "    \"Inverted normals (pointing inward instead of outward)\",\n",
    "    \"Zero-length normals (degenerate triangles)\",\n",
    "    \"Inconsistent normal directions across mesh\",\n",
    "    \"Incorrect winding order causing wrong normals\"\n",
    "]\n",
    "\n",
    "for issue in issues:\n",
    "    print(f\"‚Ä¢ {issue}\")"
   ]
