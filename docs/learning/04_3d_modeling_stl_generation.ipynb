    "class AdvancedSTLGenerator:\n",
    "    \"\"\"Advanced STL generation with enhanced features.\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        self.vertices = []\n",
    "        self.triangles = []\n",
    "        self.normals = []\n",
    "    \n",
    "    def add_triangle(self, v1, v2, v3, auto_normal=True):\n",
    "        \"\"\"Add a triangle with optional automatic normal calculation.\"\"\"\n",
    "        if auto_normal:\n",
    "            # Calculate normal using cross product\n",
    "            edge1 = np.array(v2) - np.array(v1)\n",
    "            edge2 = np.array(v3) - np.array(v1)\n",
    "            normal = np.cross(edge1, edge2)\n",
    "            \n",
    "            # Normalize\n",
    "            length = np.linalg.norm(normal)\n",
    "            if length > 1e-10:\n",
    "                normal = normal / length\n",
    "            else:\n",
    "                normal = np.array([0, 0, 1])  # Default up vector\n",
    "        else:\n",
    "            normal = np.array([0, 0, 1])  # Default normal\n",
    "        \n",
    "        self.triangles.append((v1, v2, v3))\n",
    "        self.normals.append(normal)\n",
    "    \n",
    "    def generate_surface_from_heightmap(self, heightmap, physical_params):\n",
    "        \"\"\"Generate surface triangles from heightmap with enhanced features.\"\"\"\n",
    "        \n",
    "        height, width = heightmap.shape\n",
    "        pixel_scale = physical_params['pixel_scale_mm']\n",
    "        min_height = physical_params['min_height_mm']\n",
    "        max_height = physical_params['max_height_mm']\n",
    "        base_thickness = physical_params['base_thickness_mm']\n",
    "        \n",
    "        print(f\"üîß Generating enhanced surface mesh...\")\n",
    "        \n",
    "        # Generate top surface\n",
    "        for i in range(height - 1):\n",
    "            for j in range(width - 1):\n",
    "                # Get normalized heights\n",
    "                h00 = heightmap[i, j]\n",
    "                h10 = heightmap[i + 1, j]\n",
    "                h01 = heightmap[i, j + 1]\n",
    "                h11 = heightmap[i + 1, j + 1]\n",
    "                \n",
    "                # Convert to physical coordinates\n",
    "                x0, x1 = i * pixel_scale, (i + 1) * pixel_scale\n",
    "                y0, y1 = j * pixel_scale, (j + 1) * pixel_scale\n",
    "                \n",
    "                z00 = h00 * (max_height - min_height) + min_height + base_thickness\n",
    "                z10 = h10 * (max_height - min_height) + min_height + base_thickness\n",
    "                z01 = h01 * (max_height - min_height) + min_height + base_thickness\n",
    "                z11 = h11 * (max_height - min_height) + min_height + base_thickness\n",
    "                \n",
    "                # Create two triangles for this quad\n",
    "                # Triangle 1\n",
    "                self.add_triangle(\n",
    "                    (x0, y0, z00),\n",
    "                    (x1, y0, z10),\n",
    "                    (x1, y1, z11)\n",
    "                )\n",
    "                \n",
    "                # Triangle 2\n",
    "                self.add_triangle(\n",
    "                    (x0, y0, z00),\n",
    "                    (x1, y1, z11),\n",
    "                    (x0, y1, z01)\n",
    "                )\n",
    "        \n",
    "        print(f\"   Generated {len(self.triangles)} surface triangles\")\n",
    "    \n",
    "    def add_base_plate(self, width, height, thickness, pixel_scale):\n",
    "        \"\"\"Add a solid base plate for stability.\"\"\"\n",
    "        \n",
    "        print(f\"üîß Adding base plate...\")\n",
    "        \n",
    "        max_x = width * pixel_scale\n",
    "        max_y = height * pixel_scale\n",
    "        base_z = thickness\n",
    "        \n",
    "        # Bottom face (two triangles)\n",
    "        self.add_triangle(\n",
    "            (0, 0, base_z),\n",
